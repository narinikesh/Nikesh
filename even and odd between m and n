#include<stdio.h>
int main()
{
	int m,n,i,rem;
	printf("enter two numbers");
	scanf("%d%d",&m,&n);
	printf("\n even numbers between %d and %d are",m,n);
	for(i=m;i<=n;i++)
	{
		rem=i%2;
		if(rem==0)
		printf("\n%d",i);
	}
	printf("\n odd numbers between %d and %d are",m,n);
	for(i=m;i<=n;i++)
	{
		rem=i%2;
		if(rem==1)
		printf("\n%d",i);
	}
	return 0;
}
